<!-- Form Wizard -->
<div class="callout callout-info">
  <div class="signup-brand">
    <img src="/assets/dummy/wastetrackinglogo.png" alt="Sign up">
  </div>
  <h1 class="text-lead">Create an account for Business Users</h1>
