  <p>Already have an account, please <a data-toggle="tab" href="#signin">Signin</a>.</p>
</div>

<form data-form="wizard" data-validate="form" role="form" class="form-horizontal">
  <div data-wizard>
    <h3>Account</h3>
    <section>
      <div class="form-group">
        <label class="col-sm-3 control-label" for="username">Enter Zip or Postal Code *</label>
        <div class="col-sm-7">
          <div class="col-sm-7">
            <div class="input-group input-group-in">
              <span class="input-group-addon text-silver">@</span>
              <input class="form-control" id="zipcode" name="username" type="text" minlength="2" style="height:auto !important; font-size: 12px;" required>

            </div>
          </div>
          <div class="col-sm-3">
            <button class="btn btn-primary btn-sm" data-refresh="#panel-withbtn">Update</button>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="passwd"></label>
        <div class="col-sm-7">
          <div class="col-sm-7">
            <div class="input-group input-group-in">
              <img src="/assets/dummy/mapnotavail.png" alt="No Map">
            </div>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="confirm_passwd">Location Verification</label>
        <div class="col-sm-7">
          <div class="col-sm-7">
            <div class="input-group input-group-in">
              <span class="input-group-addon text-silver"><i class="fa fa-rss"></i></span>
              <input class="form-control" id="" name="location" type="text">
            </div>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="confirm_passwd"></label>
        <div class="col-sm-7">
          <div class="col-sm-7">
            <div class="">
              <button class="btn btn-primary btn-sm" data-refresh="#panel-withbtn">Submit</button>
              <button class="btn btn-warning btn-sm" data-refresh="#panel-withbtn">No, try Again</button>

            </div>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="clearfix"></div>
    </section><!-- /step 1 -->

    <h3>Profile</h3>
    <section>

      <div class="form-group">
        <label class="col-sm-3 control-label" for="email">Email *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-envelope-o"></i></span>
            <input class="form-control" id="email" name="email" type="email" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="email">Confirm Email *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-envelope-o"></i></span>
            <input class="form-control" id="Confirmemail" name="email" type="email" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="passwd">Password *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-rss"></i></span>
            <input class="form-control" id="passwd" name="passwd" type="password" minlength="2" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="confirm_passwd">Confirm Password *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-rss"></i></span>
            <input class="form-control" id="confirm_passwd" name="confirm_passwd" type="password" equalTo="#passwd">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="firstname">First name *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-user"></i></span>
            <input class="form-control" id="firstname" name="firstname" type="text" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="lastname">Last name *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-user"></i></span>
            <input class="form-control" id="lastname" name="lastname" type="text" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="lastname">Title/Position *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-user"></i></span>
            <input class="form-control" id="titleposition" name="lastname" type="text" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="lastname">Company Name *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-building-o"></i></span>
            <input class="form-control" id="company" name="lastname" type="text" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="lastname">Company Website *</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-flag-o"></i></span>
            <input class="form-control" id="website" name="lastname" type="text" required>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">Address</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-road"></i></span>
            <input class="form-control" id="address" name="address" type="text">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">Suite/Unit</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-road"></i></span>
            <input class="form-control" id="suite" name="address" type="text">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">City</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-road"></i></span>
            <input class="form-control" id="city" name="address" type="text">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">State</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
            <span class="input-group-addon text-silver"><i class="fa fa-road"></i></span>
            <input class="form-control" id="State" name="address" type="text">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">Zipcode/Postal code</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
                                            <span class="input-group-addon text-silver">
                                                <i class="fa fa-road"></i>
                                            </span>
            <input id="mask-zip" class="form-control" type="text" placeholder="00000-000" data-mask="zip">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">How you heard about us?</label>
        <div class="col-sm-7">
          <div class="input-group input-group-in">
                                             <span class="input-group-addon text-silver">
													<i class="fa fa-question-circle"></i>
											  </span>
            <input id="howuhear" class="form-control" type="text" placeholder="(000) 000-0000" data-mask="phone_us">
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">Domain Name</label>
        <div class="col-sm-7">
          <div class="col-sm-10" style="padding-left:0 !important;">
            <div class="input-group input-group-in">
                                             <span class="input-group-addon text-silver">
													<i class="fa fa-cloud"></i>
											  </span>
              <input id="domainname" class="form-control" type="text" placeholder="(000) 000-0000" data-mask="phone_us">
            </div>
          </div>
          <div class="col-sm-2" style="padding-left:0 !important;">
            <button class="btn btn-primary btn-sm" style="line-height:1.9 !important" data-refresh="#panel-withbtn">Check availability</button>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address"></label>
        <div class="col-sm-7">
          WasteTracking.com/xxxxxxx
        </div>
      </div><!-- /form-group -->
    </section><!-- /step 2 -->

    <h3>Pricing</h3>
    <section>

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address">Wgu governed by your agency</label>
        <div class="col-sm-7">
          <div class="col-sm-10" style="padding-left:0 !important;">
            <div class="input-group input-group-in">
                                             <span class="input-group-addon text-silver">
													<i class="fa fa-calculator"></i>
											  </span>
              <input id="calculate" class="form-control" type="text" placeholder="1" data-mask="phone_us">
            </div>
          </div>
          <div class="col-sm-2" style="padding-left:0 !important;">
            <button class="btn btn-primary btn-sm" style="line-height:1.9 !important" data-refresh="#panel-withbtn">Calculate</button>
          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address"></label>
        <div class="col-sm-7">
          Based on 1 Wgu for <b>American Canyon</b> your pricing options are listed below. <br/>

          Pricing include monthly hosting, maintenence and support for staff and wgu
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" style="padding-top:0 !important" for="address">Pricing Options</label>
        <div class="col-sm-7">
          <div class="">
            <input id="business_order_value_9.99" class="business_monthly_order" type="radio" value="9.99" name="business_order_value" style="vertical-align:top;">
            Monthly USD is
            <span for="business_order_value" style="padding-right:20px;">9.99</span>


            <input id="business_order_value_29.99" class="business_monthly_order" type="radio" value="9.99" name="business_order_value" style="vertical-align:top;">
            Quarterly USD is
            <span for="business_order_value" style="padding-right:20px;"> 29.97</span>

            <input id="business_order_value_109.89" class="business_monthly_order" type="radio" value="9.99" name="business_order_value" style="vertical-align:top;">
            Annually USD is
            <span for="business_order_value" style="padding-right:20px;"> 109.89</span>

          </div>
        </div>
      </div><!-- /form-group -->

      <div class="form-group">
        <label class="col-sm-3 control-label" for="address"  style="padding-top:0 !important">Payment Options</label>
        <div class="col-sm-7">
          <div class="">
            <input id="business_order_value_109.89" class="business_monthly_order" type="radio" value="9.99" name="business_order_value" style="vertical-align:top;">
            Pay By Credit Card
            <span for="business_order_value" style="padding-right:20px;"></span>

            <input id="business_order_value_109.89" class="business_monthly_order" type="radio" value="9.99" name="business_order_value" style="vertical-align:top;">
            Pay By Invoice


          </div>
        </div>
      </div><!-- /form-group -->


      <div class="form-group">
        <label class="col-sm-3 control-label" for="address"></label>
        <div class="col-sm-7">

          <button class="btn btn-primary btn-sm" style="line-height:1.9 !important" data-refresh="#panel-withbtn">Submit</button>

        </div>
      </div><!-- /form-group -->

    </section><!-- /step 3 -->

    <h3>Finish</h3>
    <section>
      <div class="form-group">
        <div class="col-sm-offset-1 col-sm-7">
          <div class="nice-checkbox">
            <input class="checkbox-o" id="term" name="term" type="checkbox" required>
            <label for="term">
              I agree with the Terms and Conditions.
            </label>

          </div>
        </div>
      </div><!-- /form-group -->
    </section><!-- /step 4 -->
  </div><!-- /wizard -->
</form><!-- /form wizard -->